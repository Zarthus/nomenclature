{
    "name": "liefland",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "@mdi/font": "^7",
        "highlight.js": "^11",
        "markdown-it": "^13",
        "markdown-it-anchor": "^8",
        "markdown-it-highlightjs": "^4",
        "tsparticles-slim": "^2",
        "vue": "^3",
        "vue-router": "^4",
        "vue3-particles": "^2",
        "vuetify": "^3"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1",
        "@tsconfig/node20": "^20",
        "@types/jsdom": "^21",
        "@types/markdown-it": "^13",
        "@types/node": "^20",
        "@vitejs/plugin-vue": "^4",
        "@vue/eslint-config-prettier": "^8",
        "@vue/eslint-config-typescript": "^12",
        "@vue/test-utils": "^2",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8",
        "eslint-plugin-vue": "^9",
        "jsdom": "^24",
        "npm-run-all": "^4",
        "prettier": "^3",
        "typescript": "^5",
        "vite": "^4",
        "vitest": "^1.1",
        "vue-tsc": "^1"
    }
}
